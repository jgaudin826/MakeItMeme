<!DOCTYPE html>
<html>
  <head>
    <title>Votes</title>
    <link rel='stylesheet' href='/stylesheets/votes.css' />
    <link href="https://fonts.googleapis.com/css2?family=Arimo:ital,wght@0,400..700;1,400..700&family=Lilita+One&display=swap" rel="stylesheet">
  </head>
  <body id="body">
    <script src="/javascripts/votes.js" type="module"></script>
    <section class="header">
      <a id="leave" class="leave">Quitter</a>
      <h2>Votez le meme !</h2>
      <p id="memeNumber"></p>
    </section>
    <section class="main">
      <img id="meme">
      <a id="download" href="" download="MemePasMal" target="_blank">Télécharger</a>
      <section id="vote" hidden>
        <a id="upvote" class="upVote">↑</a>
        <a id="novote" class="mouaisVote">Mouais</a>
        <a id="downvote" class="downVote">↓</a>
      </section>
      <p id="timer"></p>
    </section>
  </body>
</html>
